<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:width="45dp"
    android:height="15dp"
    android:viewportWidth="60.0"
    android:viewportHeight="20.0"
    app:vc_viewportHeight="20"
    app:vc_viewportWidth="60">
    <path
        android:name="circle_extending"
        android:pathData="@string/circle_left"
        android:fillAlpha="0.30"
        android:fillColor="#ffffff"
        app:vc_pathData="@string/circle_left"
        app:vc_fillColor="#ffffff"
        app:vc_fillAlpha="0.30"/>
    <path
        android:name="circle_moving"
        android:pathData="@string/circle_left"
        android:fillAlpha="0.85"
        android:fillColor="#ffffff"
        app:vc_pathData="@string/circle_left"
        app:vc_fillColor="#ffffff"
        app:vc_fillAlpha="0.85"/>
    <path
        android:pathData="@string/circle_left"
        android:fillAlpha="0.30"
        android:fillColor="#ffffff"
        app:vc_pathData="@string/circle_left"
        app:vc_fillColor="#ffffff"
        app:vc_fillAlpha="0.30"/>
    <path
        android:pathData="@string/circle_right"
        android:fillAlpha="0.30"
        android:fillColor="#ffffff"
        app:vc_pathData="@string/circle_right"
        app:vc_fillColor="#ffffff"
        app:vc_fillAlpha="0.30"/>
</vector>
